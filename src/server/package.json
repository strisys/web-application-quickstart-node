{
  "name": "server-api",
  "version": "0.0.0-beta-0",
  "description": "Sample server API application",
  "main": "./src/js/index.ts",
  "typings": "./src/js/index.d.ts",
  "private": true,
  "scripts": {
    "install-packages-server": "npm install file:../model/server --force",
    "install-packages": "npm-run-all install-packages-server",
    "clean-js": "rimraf ./src/js",
    "tsc": "tsc",
    "transpile": "npm-run-all install-packages clean-js tsc",
    "run-browser": "start http://localhost:3000/api/v1/customers",
    "run-server": "start node ./src/js/index.js",
    "mocha": "mocha --reporter spec ./src/js/test/**/*.spec.js",
    "postinstall": "npm-run-all test",
    "test": "npm-run-all transpile mocha",
    "start": "npm-run-all --parallel transpile run-browser --sequential run-server"
  },
  "author": "Stephen Trudel",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "~1.4.6",
    "dotenv": "^10.0.0",
    "express": "~4.17.2",
    "express-session": "~1.17.2",
    "http-status-codes": "~2.2.0",
    "model-server": "file:../model/server",
    "passport": "~0.5.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "~4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "~17.0.8",
    "chai": "^4.3.6",
    "concurrently": "^7.0.0",
    "mocha": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.4"
  }
}
