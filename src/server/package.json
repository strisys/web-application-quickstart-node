{
  "name": "server-api",
  "version": "0.0.0",
  "description": "Sample server API application",
  "main": "./src/js/index.ts",
  "types": "./src/js/index.d.ts",
  "private": true,
  "scripts": {
    "install-packages": "npm install file:../model/server --force",
    "clean-js": "rimraf ./src/js",
    "tsc": "tsc",
    "transpile": "npm-run-all clean-js tsc",
    "run-browser": "start http://localhost:3000/api/v1.0/query/sales",
    "run-server-dev": "start node ./src/js/index.js",
    "run-server": "node ./src/js/index.js",
    "mocha": "mocha --exit --reporter tap \"./src/js/test/**/*.spec.js\"",
    "postinstall": "npm-run-all test",
    "test": "npm-run-all transpile mocha",
    "start-dev": "npm-run-all transpile run-server-dev",
    "start-all": "npm-run-all --parallel transpile run-browser --sequential run-server",
    "start": "npm-run-all run-server",
    "run-interactive-api-call-script": "start ts-node ./src/ts/scripts/interactive-api-call/index.ts",
    "run-api-call-script": "npm-run-all --sequential transpile run-server-dev run-interactive-api-call-script"
  },
  "author": "Stephen Trudel",
  "license": "ISC",
  "dependencies": {
    "@azure/msal-node": "^1.14.5",
    "cookie-parser": "~1.4.6",
    "express": "~4.17.2",
    "express-flash": "~0.0.2",
    "express-session": "~1.17.2",
    "http-status-codes": "~2.2.0",
    "model-server": "file:../model/server",
    "morgan": "~1.10.0",
    "passport": "0.4.0",
    "passport-azure-ad": "4.3.4",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@azure/identity": "^3.0.0",
    "@types/chai": "^4.3.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "~4.17.13",
    "@types/express-flash": "^0.0.2",
    "@types/express-session": "^1.17.4",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "~17.0.8",
    "@types/passport": "^1.0.11",
    "@types/passport-azure-ad": "^4.3.1",
    "chai": "^4.3.6",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.5",
    "mocha": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.6.0",
    "typescript": "~4.8.4"
  }
}
