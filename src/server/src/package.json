{
  "name": "server-api",
  "version": "0.0.0-beta-0",
  "description": "Sample server API application",
  "main": "./js/index.ts",
  "typings": "./js/index.d.ts",
  "private": true,
  "scripts": {
    "postinstall": "npm-run-all start",
    "install-core-packages-fs": "npm install file:../../model/server/src --force --no-save",
    "clean-js": "rimraf ./js",
    "tsc": "tsc",
    "run-browser": "start http://localhost:3000/api/v1/customers",
    "run-server": "node ./js/index.js",
    "start": "npm-run-all --parallel install-core-packages-fs transpile run-browser --sequential run-server",
    "transpile": "npm-run-all clean-js tsc",
    "mocha": "mocha --reporter spec ./js/test/**/*.spec.js"
  },
  "author": "Stephen Trudel",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "~1.4.6",
    "dotenv": "^10.0.0",
    "express": "~4.17.2",
    "express-session": "~1.17.2",
    "http-status-codes": "~2.2.0",
    "passport": "~0.5.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "~4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "~17.0.8",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.4"
  }
}
