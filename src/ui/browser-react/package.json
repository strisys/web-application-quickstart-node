{
  "name": "web-app-quickstart",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "install-packages": "^0.2.5",
    "model-client": "file:../../model/client",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "install-packages": "npm install file:../../model/client --force",
    "copy-build-server": "copyfiles -u 1 -a ./build/**/* ./build/js/*.* ./build/css/*.* ./build/media/*.* ../../server/public",
    "clean-public": "rimraf ../../server/public",
    "clean-build": "rimraf ./build",
    "clean-src-js": "rimraf ./src/js",
    "tsc": "tsc",
    "transpile": "npm-run-all clean-src-js tsc",
    "webpack-prd": "webpack-cli --mode=production --env isProd=0 printConfig=0 analyze=0",
    "webpack-dev": "webpack-cli --mode=production --env isProd=0 printConfig=0 analyze=0",
    "webpack-dev-watch": "webpack-cli --mode=development --env isProd=0 printConfig=0 analyze=0 --watch",
    "bundle-dev": "npm-run-all transpile clean-build clean-public webpack-dev",
    "build": "npm-run-all install-packages transpile bundle-dev copy-build-server",
    "run-server": "concurrently \"cd ../../server && npm run run-server && cd ../../..\"",
    "run-browser": "start http://localhost:3000",
    "start": "npm-run-all build --parallel run-server run-browser"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "browserslist": "^4.20.0",
    "concurrently": "^7.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.2.8",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  }
}
