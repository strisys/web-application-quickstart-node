{
  "name": "model-server",
  "version": "1.0.0",
  "description": "Sample server model with data sourced from a relational or document database",
  "main": "./src/js/index.js",
  "typings": "./src/js/index.d.ts",
  "private": true,
  "scripts": {
    "install-packages-core": "npm install file:../core --force",
    "install-packages": "npm-run-all install-packages-core",
    "clean-js": "rimraf ./src/js",
    "tsc": "tsc",
    "transpile": "npm-run-all install-packages clean-js tsc",
    "mocha": "mocha --exit --reporter spec ./src/js/test/**/*.spec.js",
    "test": "npm-run-all transpile mocha",
    "postinstall": "npm-run-all test"
  },
  "author": "Stephen Trudel",
  "license": "ISC",
  "engines": {
    "node": "<=18.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.21",
    "chai": "^4.3.4",
    "concurrently": "^7.0.0",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.6.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@azure/identity": "^2.0.5",
    "@azure/keyvault-secrets": "^4.4.0",
    "dotenv": "^16.0.1",
    "model-core": "file:../core"
